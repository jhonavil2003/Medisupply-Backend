{
    "executionRoleArn": "arn:aws:iam::994617978174:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 3001,
            "protocol": "tcp",
            "containerPort": 3001
          }
        ],

        "image": "<IMAGE1_NAME>",
        "essential": true,
        "name": "container-catalog-service"
      }
    ],

    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "task-catalog-service"
  }